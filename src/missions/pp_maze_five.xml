<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<Mission xmlns="http://ProjectMalmo.microsoft.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<About>
		<Summary>Maze Five</Summary>
	</About>

	<ServerSection>
		<ServerInitialConditions>
			<Time>
				<StartTime>12000</StartTime>
				<AllowPassageOfTime>false</AllowPassageOfTime>
			</Time>
			<AllowSpawning>false</AllowSpawning>
		</ServerInitialConditions>
	<ServerHandlers>
		<FlatWorldGenerator generatorString="3;7,44*49,73,35:1,159:4,95:13,35:13,159:11,95:10,159:14,159:6,35:6,95:6;12;"/>
		<DrawingDecorator>
			<!-- Draw the lengthwise walls -->
			<DrawCuboid x1="-3" y1="55" z1="25" x2="-3" y2="75" z2="-30" type="obsidian"/>
			<DrawCuboid x1="3" y1="55" z1="25" x2="3" y2="75" z2="-30" type="obsidian"/>

			<!-- Draw the widthwise walls -->                        
			<DrawCuboid x1="-3" y1="55" z1="25" x2="3" y2="75" z2="25" type="obsidian"/>
			<DrawCuboid x1="-3" y1="55" z1="-30" x2="3" y2="75" z2="-30" type="obsidian"/>

			<!-- Draw a path over the pitfall -->
			<DrawCuboid x1="-2" y1="55" z1="8" x2="-2" y2="45" z2="8" type="air" />
			<DrawCuboid x1="2" y1="55" z1="8" x2="2" y2="45" z2="8" type="air" />
			<DrawBlock x="-1" y="56" z="9" type="glass"/>
			<DrawBlock x="1" y="56" z="9" type="glass"/>
			<DrawBlock x="0" y="57" z="8" type="glass"/>
			<DrawBlock x="-1" y="56" z="7" type="glass"/>
			<DrawBlock x="1" y="56" z="7" type="glass"/>

			<!-- Draw a pitfall and some lava -->
			<DrawCuboid x1="-1" y1="55" z1="9" x2="1" y2="44" z2="7" type="air"/>
			<DrawCuboid x1="-1" y1="45" z1="9" x2="1" y2="44" z2="7" type="lava"/>

			<!-- Draw an elevated platforming type scenario haha -->
			<!-- There is no escape the only way is up... -->
			<DrawCuboid x1="-2" y1="55" z1="5" x2="2" y2="44" z2="-7" type="air"/>
			<DrawCuboid x1="-2" y1="45" z1="5" x2="2" y2="44" z2="-7" type="lava"/>

			<!-- Draw the platforming nightmare -->
			<DrawBlock x="0" y="56" z="5" type="obsidian"/>
			<DrawBlock x="0" y="57" z="4" type="obsidian"/>
			<DrawBlock x="0" y="58" z="3" type="obsidian"/>
			<DrawBlock x="0" y="59" z="2" type="obsidian"/>
			<DrawCuboid x1="-1" y1="60" z1="1" x2="1" y2="60" z2="-5" type="obsidian"/>
			<DrawBlock x="-1" y="60" z="1" type="air"/>
			<DrawBlock x="-1" y="60" z="-2" type="air"/>
			<DrawBlock x="1" y="60" z="0" type="air"/>
			<DrawBlock x="0" y="60" z="1" type="air"/>
			<DrawBlock x="0" y="60" z="-4" type="air"/>
			<DrawCuboid x1="-1" y1="59" z1="-6" x2="-1" y2="55" z2="-6" type="obsidian"/>
			<DrawCuboid x1="0" y1="58" z1="-7" x2="0" y2="55" z2="-7" type="obsidian"/>
			<DrawCuboid x1="1" y1="57" z1="-8" x2="-1" y2="55" z2="-8" type="obsidian"/>
			<DrawCuboid x1="0" y1="56" z1="-9" x2="0" y2="55" z2="-9" type="obsidian"/>

			<!-- Draw the goal and start -->
			<DrawBlock x="0" y="55" z="-29" type="diamond_block"/>
			<DrawBlock x="0" y="55" z="24" type="gold_block"/>

			<!-- Draw the starting alcove -->
			<DrawCuboid x1="-1" y1="55" z1="24" x2="-1" y2="57" z2="18" type="obsidian"/>
			<DrawCuboid x1="1" y1="55" z1="24" x2="1" y2="57" z2="18" type="obsidian"/>
			<DrawCuboid x1="0" y1="58" z1="24" x2="0" y2="58" z2="18" type="obsidian"/>
		</DrawingDecorator>
		<ServerQuitFromTimeUp timeLimitMs="60000"/>
		<ServerQuitWhenAnyAgentFinishes/>
		</ServerHandlers>
	</ServerSection>

	<AgentSection mode="Survival">
		<Name>Obstacle Course Agent</Name>
		<AgentStart>
			<Placement x="0.5" y="56" z="24.5" yaw="180"/>
		</AgentStart>
		<AgentHandlers>
		<ObservationFromFullStats/>
		<ObservationFromGrid>
			<Grid name="SubFloor">
				<min x="-1" y="-2" z="-1"/>
				<max x="1" y="-2" z="1"/>
			</Grid>
			<Grid name="Floor">
				<min x="-1" y="-1" z="-1"/>
				<max x="1" y="-1" z="1"/>
			</Grid>
			<Grid name="Level">
				<min x="-1" y="0" z="-1"/>
				<max x="1" y="0" z="1"/>
			</Grid>
			<Grid name="Roof">
				<min x="-1" y="1" z="-1"/>
				<max x="1" y="1" z="1"/>
			</Grid>
			<Grid name="SuperRoof">
				<min x="-1" y="2" z="-1"/>
				<max x="1" y="2" z="1"/>
			</Grid>
		</ObservationFromGrid>
		<AbsoluteMovementCommands/>
		<AgentQuitFromTouchingBlockType>
			<Block type="diamond_block"/>
		</AgentQuitFromTouchingBlockType>
		</AgentHandlers>
	</AgentSection>
</Mission>